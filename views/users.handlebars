<div>
	<h1>Users</h1>
	{{#if userData.admin}}
  <a href="/adduser" class="blue_button">Add User</a>
	<ul class="customList">
		{{#each users}}
		<a href="/userprofiles/{{idNumber}}"><li><b>Name&nbsp;</b>{{ firstName }} &nbsp;{{ lastName }}&nbsp;<b>Degree&nbsp;</b> {{degree}} &nbsp; <a href="/deleteuserhandler/{{idNumber}}" class="orange_button">Delete</a>&nbsp; <a href="/edituser/{{idNumber}}" class="blue_button">Edit User</a></li></a>
		{{/each}}
	</ul>
  {{else}}
	<ul class="customList">
		{{#each users}}
		<a href="/userprofiles/{{idNumber}}"><li><b>Name&nbsp;</b>{{ firstName }} &nbsp;{{ lastName }}&nbsp;<b>Email&nbsp;</b> {{email}}</li></a>
		{{/each}}
	</ul>
  {{/if}}
</div>
